<?xml version="1.0" encoding="UTF-8"?>
<manifest>
<!-- remotes -->
  <remote name="github-ssh"
           fetch="ssh://git@github.com" />
  <remote name="github-git"
           fetch="git://github.com" />
  <remote name="github-https"
           fetch="https://github.com" />
  <remote  name="github-http"
           fetch="http://github.com" />

<!-- default values -->
  <default revision="GLES2"
           remote="github-git" />

<!-- andengine core + extensions -->
  <project path="AndEngine"
           name="nicolasgramlich/AndEngine.git" />
  <project path="AndEngineLiveWallpaperExtension"
           name="nicolasgramlich/AndEngineLiveWallpaperExtension.git" />
  <project path="AndEngineTexturePackerExtension"
           name="nicolasgramlich/AndEngineTexturePackerExtension.git" />
  <project path="AndEngineTest"
           name="nicolasgramlich/AndEngineTest.git" />
  <!-- eagle has some specific stuff done inf TMX extension -->
  <project path="AndEngineTMXTiledMapExtension"
           name="nazgee/AndEngineTMXTiledMapExtension.git" />
  <project path="AndEngineSVGTextureRegionExtension"
           name="nicolasgramlich/AndEngineSVGTextureRegionExtension.git" />
  <project path="AndEnginePhysicsBox2DExtension"
           name="nicolasgramlich/AndEnginePhysicsBox2DExtension.git" />
  <project path="AndEngineMultiplayerExtension"
           name="nicolasgramlich/AndEngineMultiplayerExtension.git" />
  <project path="AndEngineAugmentedRealityExtension"
           name="nicolasgramlich/AndEngineAugmentedRealityExtension.git" />
  <project path="AndEngineExamples"
           name="nicolasgramlich/AndEngineExamples.git" />
  <project path="AndEngineMODPlayerExtension"
           name="nicolasgramlich/AndEngineMODPlayerExtension.git"
           revision="master" />

<!-- application-specific projects -->
  <project path="eagle"
           name="nazgee/eagle.git"
           revision="master"
           remote="github-ssh" />
  <project path="utils"
           name="nazgee/ae-utils.git"
           revision="master"
           remote="github-ssh" />
</manifest>
