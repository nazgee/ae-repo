<?xml version="1.0" encoding="UTF-8"?>
<manifest>
<!-- remotes -->
  <remote  name="github-ssh"
           fetch="ssh://git@github.com" />
  <remote  name="github-git"
           fetch="git://github.com" />
  <remote  name="github-https"
           fetch="https://github.com" />
  <remote  name="github-http"
           fetch="http://github.com" />

<!-- default values -->
  <default revision="GLES2-AnchorCenter-gradle"
           remote="github-http" />

<!-- andengine core + extensions -->
  <project path="AndEngine"
           name="nazgee/AndEngine.git" />
  <project path="AndEnginePhysicsBox2DExtension"
           name="nazgee/AndEnginePhysicsBox2DExtension.git"
           revision="GLES2-AnchorCenter-latest-B2D-gradle" />
  <project path="AndEngineDebugDrawExtension"
           name="nazgee/AndEngineDebugDrawExtension.git"
           revision="GLES2-AnchorCenter-latest-B2D-gradle" />
  <project path="AndEngineRubeLoaderExtension"
           name="nazgee/AndEngineRubeLoaderExtension.git"
           remote="github-http"
           revision="master-gradle" />
  <project path="AndEngineLimbo"
           name="nazgee/AndEngineLimbo.git"
           remote="github-http"
           revision="master-gradle" />

<!-- application-specific projects -->
  <project path="breaker"
           remote="github-http"
           name="nazgee/rube.git"
           revision="breaker-gradle" />
</manifest>
